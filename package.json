{
  "name": "portfolio_website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build || npm run build:local",
    "build:local": "powershell -ExecutionPolicy Bypass -File ./scripts/build-local.ps1",
    "build:clean": "powershell -ExecutionPolicy Bypass -File ./scripts/build-clean.ps1",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -f"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "vite": "^7.2.4"
  }
}
